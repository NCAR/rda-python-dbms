{
  "header": {
    "author": "Zaihua Ji, zji@ucar.edu",
    "date": "02/09/2026",
    "description": "table of tarred TACC data file information"
  },
  "database": "rdadb",
  "schema": "dssdb",
  "name": "tfile",
  "pkey": "tid",
  "column": [
    {
      "name": "tid",
      "type": "serial",
      "pkey": "PRIMARY KEY",
      "notnull": "NOT NULL",
      "comment": "auto generated TACC file id, internal use"
    },
    {
      "name": "tfile",
      "type": "varchar(255)",
      "comment": "file name under TACC home directory"
    },
    {
      "name": "uid",
      "type": "int",
      "comment": "user.uid who archived the file"
    },
    {
      "name": "dsid",
      "type": "varchar(9)",
      "comment": "reference to dataset.dsid"
    },
    {
      "name": "data_size",
      "type": "bigint",
      "comment": "number of bytes"
    },
    {
      "name": "date_created",
      "type": "date",
      "comment": "date file created"
    },
    {
      "name": "time_created",
      "type": "time",
      "comment": "time file created"
    },
    {
      "name": "date_modified",
      "type": "date",
      "comment": "date file last modified"
    },
    {
      "name": "time_modified",
      "type": "time",
      "comment": "time file last modified"
    },
    {
      "name": "data_format",
      "type": "varchar(128)",
      "comment": "the data format for NetCDF, GRIB, and etc."
    },
    {
      "name": "file_format",
      "type": "varchar(80)",
      "comment": "the file format for tar or compress"
    },
    {
      "name": "checksum",
      "type": "varchar(32)",
      "comment": "md5 checksum of saved data file"
    },
    {
      "name": "status",
      "type": "char(1)",
      "default": "'N'",
      "comment": "Noarchive, Archived or Tarred"
    },
    {
      "name": "disp_order",
      "type": "int",
      "comment": "display order of the files in a dataset"
    },
    {
      "name": "wcount",
      "type": "bigint",
      "default": "0",
      "comment": "if > 0, tar member wfile count"
    },
    {
      "name": "dsids",
      "type": "text",
      "comment": "comma-delimited additional dataset Ids"
    },
    {
      "name": "note",
      "type": "text",
      "comment": "description of this file"
    }
  ],
  "index": [
    {
      "name": "tfile_idx_1",
      "column": "tfile",
      "unique": "UNIQUE"
    },
    {
      "name": "tfile_idx_2",
      "column": "dsid"
    },
    {
      "name": "tfile_idx_3",
      "column": "status"
    }
  ],
  "comment": "TACC DATA FILES"
}